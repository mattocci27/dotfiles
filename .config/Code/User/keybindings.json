// Place your key bindings in this file to override the defaults
[
    {
        "key": "down",
        "command": "cursorDown",
        "when": "textInputFocus"
    },
    {
        "key": "up",
        "command": "cursorUp",
        "when": "textInputFocus"
    },
    {
      "key": "ctrl+shift+m", 
      "command": "markdown.showPreviewToSide", 
      "when": "editorLangId == markdown"
    }
]

